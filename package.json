{
  "name": "amp-affiliately-jekyll-theme",
  "description": "AMP Affiliately is an AMP-ready Jekyll theme for your blogs and websites.",
  "version": "3.6.0",
  "scripts": {
    "install": "bundle install",
    "start": "npm run serve",
    "serve": "bundle exec jekyll serve --livereload",
    "serve:trace": "bundle exec jekyll serve --trace --livereload",
    "lint": "markdownlint-cli2 ./_posts/**/*.md",
    "build": "cross-env NODE_ENV=production npm run build:css && cross-env JEKYLL_ENV=production bundle exec jekyll build && gulp build",
    "build:dev": "npm run build:css && bundle exec jekyll build && gulp build",
    "build:css": "npm run compile:scss && gulp minifyCSS",
    "test": "gulp validate --ignore-files=search.html",
    "test:dev": "gulp validate",
    "deploy:status": "gh workflow view jekyll-build.yml",
    "compile:scss": "sass --no-charset _sass/:_includes/css/",
    "minify:css": "gulp minifyCSS",
    "watch:all": "npm-run-all --parallel watch:scss watch:css",
    "watch:scss": "sass --no-charset --watch _sass/:_includes/css/",
    "watch:css": "gulp",
    "update:browserslist": "npx browserslist@latest --update-db"
  },
  "homepage": "https://github.com/chriskyfung/amp-affiliately-jekyll-theme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chriskyfung/amp-affiliately-jekyll-theme.git"
  },
  "bugs": {
    "url": "https://github.com/chriskyfung/amp-affiliately-jekyll-theme/issues"
  },
  "keywords": [
    "jekyll",
    "theme",
    "jekyll-theme",
    "github-pages",
    "amp",
    "jekyll-themes",
    "accelerated-mobile-pages",
    "blog-theme",
    "responsive-theme",
    "amp-jekyll",
    "jekyll-theme-amp"
  ],
  "author": "Chris K.Y. FUNG (https://chriskyfung.github.io/)",
  "license": "MIT",
  "engines": {
    "node": ">=18.12.1",
    "npm": ">=9.1.2"
  },
  "type": "module",
  "devDependencies": {
    "@ampproject/toolbox-optimizer": "^2.10.1",
    "amphtml-validator": "^1.0.38",
    "ansi-colors": "^4.1.3",
    "cross-env": "^10.1.0",
    "fancy-log": "^2.0.0",
    "gulp": "^5.0.1",
    "gulp-csso": "^4.0.1",
    "gulp-ext-replace": "^0.3.0",
    "gulp-html-minifier-terser": "^7.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "npm-run-all": "^4.1.5",
    "sass": "^1.77.2",
    "through2": "^4.0.2",
    "yargs": "^18.0.0"
  },
  "overrides": {
    "@ampproject/toolbox-optimizer": {
      "postcss": "^8.4.31",
      "terser": ">=5.14.2"
    }
  },
  "resolutions": {
    "node-sass": "https://registry.npmjs.org/node-sass/-/node-sass-7.0.0.tgz"
  }
}
