{
  "grammarly.domain": "technical",
  "grammarly.userWords": [
    "css",
    "jekyll",
    "html"
  ],
  "grammarly.audience": "general",
  "grammarly.hideUnavailablePremiumAlerts": true,
  "liveServer.settings.proxy": {
    "enable": true,
    "baseUri": "/amp-affiliately-jekyll-theme",
    "proxyUri": "http://127.0.0.1:5500/_site"
  },
  "frontMatter.framework.id": "jekyll",
  "frontMatter.framework.startCommand": "bundle exec jekyll serve --trace --livereload",
  "frontMatter.preview.host": "http://127.0.0.1:4000/amp-affiliately-jekyll-theme/",
  "git-graph.customBranchGlobPatterns": [
    {
      "name": "No gh-pages",
      "glob": "heads/[^g][^h]*[^s]"
    },
    {
      "name": "No origin/gh-pages",
      "glob": "remotes/origin/[^g][^h]*[^s]"
    },
    {
      "name": "Branches",
      "glob": "*/*"
    },
    {
      "name": "Stashes",
      "glob": "stash"
    },
    {
      "name": "Tags",
      "glob": "tags/*"
    }
  ],
  "git-graph.repository.onLoad.showSpecificBranches": [
    "master",
    "remotes/origin/master",
    "--glob=*/*",
    "--glob=stash",
    "--glob=tags/*"
  ],
  "html.validate.styles": false,
  "markdownlint.config": {
    "MD046": {
      "style": "fenced"
    }
  },
  "liquid.engine": "jekyll",
  "liveSassCompile.settings.formats": [
    {
        "format": "expanded",
        "extensionName": ".css",
        "savePath": "/_includes/css"
    },
    {
        "extensionName": ".min.css",
        "format": "compressed",
        "savePath": "/_includes/css"
    }
  ]
}
