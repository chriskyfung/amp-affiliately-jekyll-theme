{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "fields": [
        {
          "title": "Layout",
          "name": "layout",
          "type": "choice",
          "choices": [
            "archive",
            "page",
            "post"
          ],
          "default": "post",
          "required": true
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "required": true
        },
        {
          "title": "Excerpt",
          "name": "excerpt",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "dateFormat": "YYYY-MM-DD HH:MM +0800",
          "default": "{{now}}",
          "isPublishDate": true,
          "required": true
        },
        {
          "title": "Last modified date",
          "name": "last_modified_at",
          "type": "datetime",
          "dateFormat": "YYYY-MM-DD HH:MM +0800",
          "isModifiedDate": true
        },
        {
          "title": "Article preview",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "boolean"
        },
        {
          "title": "Category",
          "name": "category",
          "type": "categories"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "jekyll",
  "frontMatter.content.publicFolder": "assets",
  "frontMatter.preview.host": "http://localhost:4000",
  "frontMatter.site.baseURL": "https://chriskyfung.github.io/",
  "frontMatter.website.host": "https://chriskyfung.github.io/amp-affiliately-jekyll-theme/",
  "frontMatter.content.pageFolders": [
    {
      "title": "posts",
      "path": "[[workspace]]/_posts",
      "previewPath": "{{fm.permalink}}"
    }
  ]
}
